all:
	gcc -c cl_image.c ../Cdemos/image.o -I../ -I./ -I/opt/AMDAPP/include -I../ -L/opt/AMDAPP/lib/x86/ -lOpenCL -o cl_image.o -std=gnu99
	gcc main.c cl_image.o ../Cdemos/image.o -I../ -I./ -I/opt/AMDAPP/include -L/opt/AMDAPP/lib/x86 -lOpenCL -o opencl -std=gnu99
run:
	./opencl
clean:
	rm *.o
	rm ./opencl
